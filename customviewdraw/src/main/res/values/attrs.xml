<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundImageViewByXfermode">
        <attr name="borderRadius" format="dimension"></attr>
        <attr name="type">
            <enum name="circle" value="0"/>
            <enum name="round" value="1"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="custume_ripple_imageview">
        <attr name="show_spacing_time" format="integer"></attr>
        <attr name="imageViewWidth" format="dimension"></attr>
        <attr name="imageViewHeigth" format="dimension"></attr>
        <attr name="background" format="reference"></attr>
        <attr name="anim" format="reference"></attr>
    </declare-styleable>
    <declare-styleable name="WaveLineView">
        <attr name="wave_first_color" format="color" />
        <attr name="wave_second_color" format="color" />
        <attr name="wave_line_width" format="dimension" />
        <attr name="wave_phase" format="float" />
        <attr name="wave_amplifier" format="float" />
        <attr name="wave_frequency" format="float" />
    </declare-styleable>
</resources>