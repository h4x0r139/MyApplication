<?xml version="1.0" encoding="utf-8"?>
<manifest package="org.yinxm.data"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="org.yinxm.data.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <provider
            android:name="org.yinxm.data.BookProvider"
            android:authorities="org.wangchenlong.book.provider"
            android:permission="org.wangchenlong.BOOK_PROVIDER"
            android:process=":provider"/>

    </application>

</manifest>